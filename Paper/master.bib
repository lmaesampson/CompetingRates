@article{crosby_uptake_2011,
	title = {Uptake of {Free} {HPV} {Vaccination} {Among} {Young} {Women}: {A} {Comparison} of {Rural} {Versus} {Urban} {Rates}},
	volume = {27},
	issn = {1748-0361},
