	author = {Metcalf, C. J. E. and Cohen, C. and Lessler, J. and McAnerney, J. M. and Ntshoe, G. M. and Puren, A. and Klepac, P. and Tatem, A. and Grenfell, B. T. and BjÃ¸rnstad, O. N.},
