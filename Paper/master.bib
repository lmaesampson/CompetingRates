@article{ferrari_episodic_2010,
	title = {Episodic outbreaks bias estimates of age-specific force of infection: a corrected method using measles as an example},
	volume = {138},
	issn = {1469-4409, 0950-2688},
	shorttitle = {Episodic outbreaks bias estimates of age-specific force of infection},
	url = {https://www.cambridge.org/core/journals/epidemiology-and-infection/article/episodic-outbreaks-bias-estimates-of-age-specific-force-of-infection-a-corrected-method-using-measles-as-an-example/DFBED3083DD9F8181AE52A44B18B24EE},
	doi = {10.1017/S0950268809990173},
	abstract = {SUMMARYUnderstanding age-specific differences in infection rates can be important in predicting the magnitude of and mortality in outbreaks and targeting age groups for vaccination programmes. Standard methods to estimate age-specific rates assume that the age-specific force of infection is constant in time. However, this assumption may easily be violated in the face of a highly variable outbreak history, as recently observed for acute immunizing infections like measles, in strongly seasonal settings. Here we investigate the biases that result from ignoring such fluctuations in incidence and present a correction based on the epidemic history. We apply the method to data from a measles outbreak in Niamey, Niger and show that, despite a bimodal age distribution of cases, the estimated age-specific force of infection is unimodal and concentrated in young children ({\textless}5 years) consistent with previous analyses of age-specific rates in the region.},
	number = {1},
	urldate = {2016-10-21TZ},
	journal = {Epidemiology \&amp; Infection},
	author = {Ferrari, M. J. and Djibo, A. and Grais, R. F. and Grenfell, B. T. and BjÃ¸rnstad, O. N.},
	month = jan,
	year = {2010},
	keywords = {Age structure, Estimation, force of infection, measles (rubeola)},
	pages = {108--116}
}

