	volume = {17},
	copyright = {Â© 2016 Nature Publishing Group, a division of Macmillan Publishers Limited. All Rights Reserved.},
	issn = {1529-2908},
	url = {http://www.nature.com/ni/journal/v17/n9/full/ni.3515.html},
	doi = {10.1038/ni.3515},
	abstract = {Zika virus (ZIKV) was discovered in 1947 and was thought to lead to relatively mild disease. The recent explosive outbreak of ZIKV in South America has led to widespread concern, with reports of neurological sequelae ranging from Guillain BarrÃ© syndrome to microcephaly. ZIKV infection has occurred in areas previously exposed to dengue virus (DENV), a flavivirus closely related to ZIKV. Here we investigated the serological cross-reaction between the two viruses. Plasma immune to DENV showed substantial cross-reaction to ZIKV and was able to drive antibody-dependent enhancement (ADE) of ZIKV infection. Using a panel of human monoclonal antibodies (mAbs) to DENV, we showed that most antibodies that reacted to DENV envelope protein also reacted to ZIKV. Antibodies to linear epitopes, including the immunodominant fusion-loop epitope, were able to bind ZIKV but were unable to neutralize the virus and instead promoted ADE. Our data indicate that immunity to DENV might drive greater ZIKV replication and have clear implications for disease pathogenesis and future vaccine programs for ZIKV and DENV.},
	language = {en},
	number = {9},
	urldate = {2016-11-28TZ},
	journal = {Nature Immunology},
	author = {Dejnirattisai, Wanwisa and Supasa, Piyada and Wongwiwat, Wiyada and Rouvinski, Alexander and Barba-Spaeth, Giovanna and Duangchinda, Thaneeya and Sakuntabhai, Anavaj and Cao-Lormeau, Van-Mai and Malasit, Prida and Rey, Felix A. and Mongkolsapaya, Juthathip and Screaton, Gavin R.},
	month = sep,
	year = {2016},
	keywords = {Cell death and immune response, Dengue virus, Viral infection},
	pages = {1102--1108}
}

@article{brauer_kermackmckendrick_2005,
