	author = {Newman, M. E. J.},
	month = oct,
	year = {2002},
	note = {arXiv: cond-mat/0205405},
	keywords = {Condensed Matter - Disordered Systems and Neural Networks}
}

@article{smieszek_mechanistic_2009,
