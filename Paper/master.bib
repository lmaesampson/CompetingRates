@article{grundmann_risk_2002,
	title = {Risk {Factors} for the {Transmission} of {Methicillin}-{Resistant} {\textbackslash}{emphStaphylococcus} aureus in an {Adult} {Intensive} {Care} {Unit}: {Fitting} a {Model} to the {Data}},
