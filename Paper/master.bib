	journal = {Journal of Population and Social Studies [JPSS]},
	author = {Kantayaporn, Tussnai and Archavanitkul, Kritaya and Peerapatanapokin, Wiwat and Disthawong, Nonglak and Singkul, Nattawut and Sinvuttaya, Sasithorn and Chinvarasopak, Pasupha and Panatanasan, Kritica},
	year = {2013},
	keywords = {EPI, Immunization, Vaccine, children of migrants},
	pages = {227--242}
}

